    <eLiterals name="SampleValues_ASC" value="3"/>
    <eLiterals name="SampleValues_DESC" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="RadioModes">
    <eLiterals name="Mode_1"/>
    <eLiterals name="Mode_2" value="1"/>
    <eLiterals name="Mode_3" value="2"/>
    <eLiterals name="Mode_4" value="3"/>
  </eClassifiers>
</ecore:EPackage>
