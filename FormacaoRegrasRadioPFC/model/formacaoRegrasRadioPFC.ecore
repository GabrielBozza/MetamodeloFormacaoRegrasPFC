    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Operation" lowerBound="1"
        eType="#//MathOp" defaultValueLiteral="plus"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Attributes" eSuperTypes="#//Operand">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//AttrTypes"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Operand"/>
  <eClassifiers xsi:type="ecore:EClass" name="Value" eSuperTypes="#//Operand"/>
  <eClassifiers xsi:type="ecore:EClass" name="Num" eSuperTypes="#//Value">
