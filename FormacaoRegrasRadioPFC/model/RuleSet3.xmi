<?xml version="1.0" encoding="UTF-8"?>
<action:RuleSet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:action="http://action/1.0"
    xsi:schemaLocation="http://action/1.0 formacaoRegrasRadioPFC.ecore">
  <rule name="RuleST1"
      priority="1">
    <leftside
        xsi:type="action:CompoundExp"
        PrecedingLogicOp="None">
      <expression
          xsi:type="action:SimpleExp"
          PrecedingLogicOp="None">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:Attributes"
            name="SampleValues_ASC"/>
        <operand
            xsi:type="action:Strings"
            value="true"/>
      </expression>
      <expression
          xsi:type="action:CompoundExp">
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="None">
          <operator
              xsi:type="action:Comparator"
              Operation="greater_equal"/>
          <operand
              xsi:type="action:Attributes"/>
          <operand
              xsi:type="action:Num"
              value="0.5"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="50.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="OpRole"/>
          <operand
              xsi:type="action:Strings"
              value="cmt"/>
        </expression>
      </expression>
    </leftside>
    <rightside
        xsi:type="action:ActionRadio"
        newState="ALERT_MODE"/>
  </rule>
</action:RuleSet>
