<?xml version="1.0" encoding="UTF-8"?>
<action:Rule
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:action="http://action/1.0"
    xsi:schemaLocation="http://action/1.0 formacaoRegrasRadioPFC.ecore"
    name="RuleTest3">
  <leftside
      xsi:type="action:CompoundExp"
      PrecedingLogicOp="None">
    <expression
        xsi:type="action:SimpleExp"
        PrecedingLogicOp="None">
      <operator
          xsi:type="action:Comparator"
          Operation="greater_equal"/>
      <operand
          xsi:type="action:Attributes"
          name="CarrierFreq"
          type="CarrierFreq"/>
      <operand
          xsi:type="action:Num"
          value="100.0"/>
    </expression>
    <expression
        xsi:type="action:SimpleExp">
      <operator
          xsi:type="action:Comparator"
          Operation="less_equal"/>
      <operand
          xsi:type="action:Attributes"
          name="SecLevel"/>
      <operand
          xsi:type="action:Num"
          value="0.5"/>
    </expression>
    <expression
        xsi:type="action:CompoundExp"
        PrecedingLogicOp="or">
      <expression
          xsi:type="action:SimpleExp"
          PrecedingLogicOp="None">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:Attributes"
            name="OpRole"
            type="OpRole"/>
        <operand
            xsi:type="action:Strings"
            value="soldier"/>
      </expression>
      <expression
          xsi:type="action:CompoundExp">
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="None">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="SampleValues_ASC"
              type="SampleValues_ASC"/>
          <operand
              xsi:type="action:Strings"
              value="true"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="SampleValues_DESC"
              type="SampleValues_DESC"/>
          <operand
              xsi:type="action:Strings"
              value="false"/>
        </expression>
      </expression>
    </expression>
  </leftside>
  <rightside
      xsi:type="action:ActionRadio"
      newState="Mode_4"/>
</action:Rule>
