<?xml version="1.0" encoding="UTF-8"?>
<action:RuleSet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:action="http://action/1.0"
    xsi:schemaLocation="http://action/1.0 formacaoRegrasRadioPFC.ecore">
  <rule name="Rule1"
      priority="1">
    <leftside
        xsi:type="action:CompoundExp"
        PrecedingLogicOp="None">
      <expression
          xsi:type="action:SimpleExp"
          PrecedingLogicOp="None">
        <operator
            xsi:type="action:Comparator"/>
        <operand
            xsi:type="action:Attributes"/>
        <operand
            xsi:type="action:Num"
            value="50.0"/>
      </expression>
      <expression
          xsi:type="action:CompoundExp"
          PrecedingLogicOp="or">
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="None">
          <operator
              xsi:type="action:Comparator"/>
          <operand
              xsi:type="action:Attributes"/>
          <operand
              xsi:type="action:Num"
              value="50.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="or">
          <operator
              xsi:type="action:Comparator"
              Operation="less"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="100.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp">
          <operator
              xsi:type="action:Comparator"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="75.0"/>
        </expression>
      </expression>
      <expression
          xsi:type="action:SimpleExp"
          PrecedingLogicOp="or">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:Attributes"
            name="OpRole"/>
        <operand
            xsi:type="action:Strings"
            value="cmt"/>
      </expression>
    </leftside>
    <rightside
        xsi:type="action:ActionRadio"
        newState="NORMAL_MODE"/>
  </rule>
  <rule name="Rule2"
      priority="2">
    <leftside
        xsi:type="action:CompoundExp"
        PrecedingLogicOp="None">
      <expression
          xsi:type="action:CompoundExp"
          PrecedingLogicOp="None">
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="None">
          <operator
              xsi:type="action:Comparator"/>
          <operand
              xsi:type="action:Attributes"/>
          <operand
              xsi:type="action:Num"
              value="50.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="or">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="100.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="100.0"/>
        </expression>
      </expression>
      <expression
          xsi:type="action:SimpleExp">
        <operator
            xsi:type="action:Comparator"
            Operation="less_equal"/>
        <operand
            xsi:type="action:Attributes"/>
        <operand
            xsi:type="action:Num"/>
      </expression>
      <expression
          xsi:type="action:SimpleExp">
        <operator
            xsi:type="action:Comparator"
            Operation="not_equal"/>
        <operand
            xsi:type="action:Attributes"
            name="SampleValues_DESC"/>
        <operand
            xsi:type="action:Strings"
            value="false"/>
      </expression>
      <expression
          xsi:type="action:CompoundExp"
          PrecedingLogicOp="or">
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="None">
          <operator
              xsi:type="action:Comparator"/>
          <operand
              xsi:type="action:Attributes"/>
          <operand
              xsi:type="action:Num"
              value="50.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp"
            PrecedingLogicOp="or">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="100.0"/>
        </expression>
        <expression
            xsi:type="action:SimpleExp">
          <operator
              xsi:type="action:Comparator"
              Operation="equal"/>
          <operand
              xsi:type="action:Attributes"
              name="CarrierFreq"/>
          <operand
              xsi:type="action:Num"
              value="100.0"/>
        </expression>
      </expression>
    </leftside>
    <rightside
        xsi:type="action:ActionRadio"
        newState="ALERT_MODE"/>
  </rule>
  <rule name="Rule3"
      priority="3">
    <leftside
        xsi:type="action:SimpleExp"
        PrecedingLogicOp="not">
      <operator
          xsi:type="action:Comparator"
          Operation="equal"/>
      <operand
          xsi:type="action:Attributes"
          name="SampleValues_ASC"/>
      <operand
          xsi:type="action:Strings"
          value="true"/>
    </leftside>
    <rightside
        xsi:type="action:ActionRadio"
        newState="FULLDUPLEX_MODE"/>
  </rule>
</action:RuleSet>
