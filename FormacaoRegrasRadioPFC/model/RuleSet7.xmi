<?xml version="1.0" encoding="UTF-8"?>
<action:RuleSet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:action="http://action/1.0"
    xsi:schemaLocation="http://action/1.0 formacaoRegrasRadioPFC.ecore">
  <rule name="RuleTest"
      priority="5">
    <leftside
        xsi:type="action:CompoundExp">
      <expression
          xsi:type="action:SimpleExp">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:BoolAttributes"/>
        <operand
            xsi:type="action:Bool"
            value="true"/>
      </expression>
      <expression
          xsi:type="action:SimpleExp"
          PrecedingLogicOp="and">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:StringAttributes"/>
        <operand
            xsi:type="action:Strings"
            value="Soldier"/>
      </expression>
    </leftside>
    <rightside
        xsi:type="action:ActionRadio"
        newState="FULLDUPLEX_MODE"/>
  </rule>
</action:RuleSet>
