<?xml version="1.0" encoding="UTF-8"?>
<action:RuleSet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:action="http://action/1.0"
    xsi:schemaLocation="http://action/1.0 formacaoRegrasRadioPFC.ecore">
  <rule name="RuleST_7"
      priority="7">
    <leftside
        xsi:type="action:CompoundExp">
      <expression
          xsi:type="action:SimpleExp">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:Attributes"
            name="SNRValues_ASC"/>
        <operand
            xsi:type="action:Strings"
            value="false"/>
      </expression>
      <expression
          xsi:type="action:SimpleExp"
          PrecedingLogicOp="and">
        <operator
            xsi:type="action:Comparator"
            Operation="equal"/>
        <operand
            xsi:type="action:Attributes"
            name="SNRValues_DESC"/>
        <operand
            xsi:type="action:Strings"
            value="false"/>
      </expression>
    </leftside>
    <rightside
        xsi:type="action:ActionRadio"
        newState="MAINTAIN_CURRENT_MODE"/>
  </rule>
</action:RuleSet>
